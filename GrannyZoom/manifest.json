{
	"manifest_version": 2,

	"name": "Granny Zoom",
	"description": "Zoom zoom",

	"version": "1.0",

	"permissions": [
		"activeTab"
	],
	
	"icons": {
		"256": "zoom256.png"
	},

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},

	"browser_action": {
		"default_popup": "popup.html"
	}
}
